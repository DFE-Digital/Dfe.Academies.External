  },
  "homepage": "https://github.com/DFE-Digital/Dfe.Academies.External/blob/master/Dfe.Academies.External.Web/CypressTests/README.md",
  "dependencies": {
    "@types/node": "^20.2.5",
    "cypress": "^13.0.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-slack-reporter": "^1.5.4",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@cypress/grep": "^4.0.1",
    "@faker-js/faker": "^8.0.2",
